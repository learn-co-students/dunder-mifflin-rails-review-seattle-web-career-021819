<h1>Belongs to dog</h1>


<%=form_for(@employee) do |f|%>
<%=f.label :name%>
<%=f.text_field :first_name%><br>
<%=f.label :title%>
<%=f.text_field :title%><br>
<%=f.label :alias%>
<%=f.text_field :alias%><br>
<label>Belongs to: </label>
<%= f.collection_select :dog_id, Dog.all, :id, :name %><br>
<%=f.submit%>

<%end%>
